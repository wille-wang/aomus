<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >


    <LinearLayout
        android:id="@+id/we"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/edittext_background"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/ic_search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_search"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:clickable="true"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:focusable="true"/>



    <EditText
        android:id="@+id/et_account"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:hint=""
        android:padding="10dp"
        android:textSize="25sp" />


        <ImageView
            android:id="@+id/ic_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_camera"
            android:layout_toLeftOf="@id/ic_search"
            android:layout_toStartOf="@id/ic_search"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:clickable="true"
            android:focusable="true"/>



    </LinearLayout>







    <LinearLayout
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/we"
        android:padding="16dp">


    <LinearLayout
        android:id="@+id/two"
        android:layout_width="0dp"
        android:layout_weight="4"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <!-- 第一行：TextView -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hi, Yu"
        android:textSize="20sp"
       />

    <!-- 第二行：ImageView 和 TextView -->
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/image01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/li0" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="22 Sept"
            android:textSize="18sp"
            android:layout_marginStart="16dp" />
    </LinearLayout>


</LinearLayout>



        <ImageView
            android:id="@+id/image02"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/p1"
            android:scaleType="centerCrop"/>

</LinearLayout>



    <!-- 包含三个TextView的水平LinearLayout -->
    <LinearLayout
        android:id="@+id/four"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:layout_below="@+id/one"
        android:layout_marginTop="20dp"
        android:gravity="center">

        <!-- 第一个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Class"
            android:textSize="20sp"
            android:layout_marginRight="10dp"
            android:drawableStart="@drawable/kk01"
            android:background="@drawable/bg_button"
            android:gravity="center"/>

        <!-- 第二个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="Events"
            android:textSize="20sp"
            android:layout_marginRight="10dp"
            android:drawableStart="@drawable/kk02"
            android:background="@drawable/bg_button1"
            android:gravity="center"/>

        <!-- 第三个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_weight="1"
            android:text="T0"
            android:textSize="20sp"
            android:background="@drawable/bg_button1"
            android:drawableStart="@drawable/kk03"
            android:gravity="center"/>
    </LinearLayout>




    <!-- 包含三个TextView的LinearLayout -->
    <LinearLayout
        android:id="@+id/five"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/four"
        android:background="#f0f0f0">

        <!-- 第一个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text=""
            android:textSize="18sp"
            android:gravity="center"
            android:layout_marginRight="20dp"
            android:background="@color/color_dcdcdc"/>

        <!-- 第二个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="4"
            android:text=""
            android:textSize="18sp"
            android:gravity="center"
            android:layout_marginRight="20dp"
            android:background="@color/color_dcdcdc"/>

        <!-- 第三个TextView -->
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text=""
            android:textSize="18sp"
            android:gravity="center"
            android:background="@color/color_dcdcdc"/>
    </LinearLayout>














    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:background="#f7f7f7"
        android:gravity="center"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/see_title_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <ImageButton
                android:id="@+id/see_title_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/home0" />

            <TextView
                android:id="@+id/see_title_text"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center_horizontal"
                android:gravity="top"
                android:text=""
                android:textColor="#82858b"
                android:textSize="13dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/chosed_title_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <ImageButton
                android:id="@+id/chosed_title_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/map0" />

            <TextView
                android:id="@+id/chosed_title_text"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center_horizontal"
                android:gravity="top"
                android:text=""
                android:textColor="#82858b"
                android:textSize="13dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/information_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <ImageButton
                android:id="@+id/information_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/shi0" />

            <TextView
                android:id="@+id/information_text"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center_horizontal"
                android:gravity="top"
                android:text=""
                android:textColor="#82858b"
                android:textSize="13dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/back_layout"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_weight="1"
            android:orientation="vertical"
            android:padding="3dp">

            <ImageButton
                android:id="@+id/back_img"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_gravity="center_horizontal"
                android:src="@drawable/wo0" />

            <TextView
                android:id="@+id/back_text"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_gravity="center_horizontal"
                android:gravity="top"
                android:text=""
                android:textColor="#82858b"
                android:textSize="13dp" />

        </LinearLayout>



    </LinearLayout>


</RelativeLayout>

